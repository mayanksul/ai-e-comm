{
  "builds": [
    {
      "src": "src/streamlit_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "maxLambdaSize": "250mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/streamlit_app.py"
    }
  ],
  "functions": {
    "src/streamlit_app.py": {
      "includeFiles": "assets/**"
    }
  }
} 