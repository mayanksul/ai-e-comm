{
  "builds": [
    {
      "src": "src/streamlit_app.py",
      "use": "@vercel/python", 
      "config": {
        "runtime": "python3.10",
        "includeFiles": [
          "assets/**",
          "data/raw/Apparels_shared.csv"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/streamlit_app.py"
    }
  ],
  "functions": {
    "api/**": {
      "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
    }
  }
}